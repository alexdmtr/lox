// Prints all the prime numbers up to N.

var N = 15;
var num = 2;

while (num <= N)
{
    var prime = true;
    if (num % 2 == 0 and num != 2)
        prime = false;
    else
    {
        var d = 3;
        while (d * d <= num and prime)
        {
            if (num % d == 0)
                prime = false;
            d = d + 2;
        }
    }

    if (prime)
        print num;
    num = num + 1;
}